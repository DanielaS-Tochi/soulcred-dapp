# Blockchain RPC Providers (Optional - public providers used as fallback)
VITE_ALCHEMY_API_KEY=your_alchemy_api_key_here
VITE_INFURA_API_KEY=your_infura_api_key_here

# WalletConnect (Optional - basic connectors used as fallback)
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# IPFS Storage (Required for file uploads)
VITE_PINATA_JWT=your_pinata_jwt_here
PINATA_JWT=your_pinata_jwt_here

# Supabase Database (Recommended for production - localStorage used as fallback)
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Feature Flags
VITE_ENABLE_MAINNET=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_BETA_FEATURES=false